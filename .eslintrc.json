{
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
       
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "strict":["error","global"],
        "no-restricted-globals": ["error", "event", "fdescribe"]
        

    }
}